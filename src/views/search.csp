
<%inc #include <json/value.h> %>

<%view _header.csp %>

<%c++ const auto projectLst = @@.get<Json::Value>("project_lst"); %>

<h1>Project Search</h1>

<ul>
    <%c++ for(const auto& project : projectLst) { %>
        <%c++ const auto projectName = project["name"].asString(); %>
        <%c++ const auto projectId = project["id"].asString(); %>
        <li><a href="/project/{%projectId%}">{%projectName%}</a></li>
    <%c++ } %>
</ul>


<%view _footer.csp %>

