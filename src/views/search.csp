
<%inc #include <json/value.h> %>

<%view _header.csp %>

<%c++ const auto projectLst = @@.get<Json::Value>("projectLst"); %>

<h1>Project Search</h1>

<ul>
    <%c++ for(const auto& project : projectLst) { %>
        <%c++ const auto project_name = project["name"].asString(); %>
        <%c++ const auto project_id = project["id"].asString(); %>
        <li><a href="/project/{%project_id%}">{%project_name%}</a></li>
    <%c++ } %>
</ul>


<%view _footer.csp %>

